  "devDependencies": {
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@types/node": "14.14.16",
    "@types/puppeteer": "5.4.2",
    "@types/react": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.14.1",
